<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>8.0</LangVersion>
    <NullableContextOptions>enable</NullableContextOptions>

    <Authors>Sarin Na Wangkanai</Authors>
    <Description>ASP.NET Core Detection Device resolver components</Description>
    <Copyright>Copyright Â© 2018 Sarin Na Wangkanai</Copyright>
    <PackageTags>aspnetcore;detection;device</PackageTags>
    <PackageProjectUrl>https://github.com/wangkanai/Detection</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com/wangkanai/Detection</RepositoryUrl>
    <RootNamespace>Wangkanai.Detection</RootNamespace>
    <PackageIcon>avatar.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Detection.Core\src\Wangkanai.Detection.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\asset\avatar.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
