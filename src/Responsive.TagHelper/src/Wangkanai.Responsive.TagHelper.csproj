<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>Sarin Na Wangkanai</Authors>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Wangkanai.Responsive.TagHelper</AssemblyName>
    <PackageId>Wangkanai.Responsive.TagHelper</PackageId>
    <PackageTags>aspnetcore;Responsive;TagHelper</PackageTags>
    <PackageProjectUrl>https://github.com/wangkanai/detection</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>git://github.com/wangkanai/detection</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>avatar.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Responsive.Core\src\Wangkanai.Responsive.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\asset\avatar.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>